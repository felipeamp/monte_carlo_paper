{
    "datasets basepath": "./datasets",
    "use all datasets": true,
    "load one dataset at a time": false,

    "criteria": [
        "Gini Gain",
        "Twoing",
        "Gain Ratio"
    ],

    "output folder": "./outputs/multiple levels experiment/monte carlo",

    "num trials": 5,
    "calculate t-test on accuracy": true,
    "t-test single-sided p-value": 0.1,

    "max depth": 5,
    "use cross-validation": true,
    "num folds": 10,
    "is stratified": true,

    "pruning parameters list": [{
        "use chi-sq test": false,

        "use monte carlo": true,
        "monte carlo parameters": {
            "upper p-value threshold": 0.4,
            "lower p-value threshold": 0.1,
            "prob monte carlo": 0.95,
            "use random order": false,
            "use one attrib per num values": false
        },

        "min num samples allowed": 1,
        "use second most freq class min samples": true,
        "second most freq class min samples": 40
    }]
}